<?xml version="1.0"?>


<group name="Arduino" description="Test Arduino board">


<description type="text">
	Arduino Serial Communication example
</description>


<example description="Serial communication">
	<module
		class="Arduino"
		name="ARDUINO"
		port="/dev/ttyACM0"
	/>
</example>

<parameter name="port"
	description="USB Port"
	type="string"
	default="/dev/ttyACM0"/>

 <input name="GOAL_POSITION" description="The goal position of the Dynamixel" />
 <input name="CURRENT_POSITION" description="The current position of the Dynamixel" />

 <output name="OUTPUT" description="Arduino serial communication output" size="1" />

